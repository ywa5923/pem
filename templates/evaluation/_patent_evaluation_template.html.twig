<table class="table table-striped table-hover table-sm table-responsive-md">

    <tr>
        <th colspan="9">
            <h4>Sectiunea A1.1: Brevete - {{ patents.totalPoints|number_format(2) }} puncte</h4>
        </th>
    </tr>
    <tr>
        <th>Nr:</th>
        <th>Titlu brevet</th>
        <th>Autori</th>
        <th>Neff</th>
        <th>Patent Type</th>
        <th>Nr inregistrare</th>
        <th>Publicat in B.O.P.I, data, pagina</th>
        <th>Nr si data deciziei de acordare</th>
        <th>Punctaj</th>

    </tr>

    {% for patent in patents.items %}
        <tr>
            <td>{{ loop.index }}</td>
            <td>{{ patent.title }}</td>
            <td>{{ patent.authors }}</td>
            <td>{{ patent.neff }}</td>
            <td>{{ patent.patentType }}</td>
            <td>{{ patent.doi }}</td>
            <td>{{ patent.journal }}</td>
            <td>{{ patent.miscellanous }}</td>
            <td>{{ patent.total|number_format(2) }}</td>
        </tr>
    {% endfor %}
</table>